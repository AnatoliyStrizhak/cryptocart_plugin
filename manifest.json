{
    "manifest_version": 2,

    "name": "CryptoCart",
    "description": "Cart",
    "version": "0.2",

    "permissions": [ "tabs", "http://*/*", "https://*/*", "storage"],

    "background": {"scripts": ["background.js"],"persistent": false},

    "description": "Pokupo CryptoCart - pay for orders in *coins",

    "browser_action": {
        "default_icon": "logo_bw.png",
        "default_title": "CryptoCart",
        "default_popup": "popup.html"
    }
}
